model Core {
  id        Int           @id @default(autoincrement())
  vendor    CoreVendor?
  series    CoreSeries?
  name      String
  socs      CoresOnSocs[]
  createdAt DateTime      @default(now()) @map("created_at")
  updatedAt DateTime      @updatedAt @map("updated_at")

  @@unique([vendor, series, name])
}
